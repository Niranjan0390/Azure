{
	"name": "df_demo",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dsExcelDemo",
						"type": "DatasetReference"
					},
					"name": "ExcelSource"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Column_1 as integer,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as integer,",
				"          Column_5 as timestamp,",
				"          Column_6 as decimal(10,0),",
				"          Column_7 as decimal(10,0),",
				"          Column_8 as decimal(10,0),",
				"          Column_9 as decimal(10,0),",
				"          Column_10 as decimal(10,0),",
				"          Column_11 as string,",
				"          Column_12 as string,",
				"          Column_13 as integer,",
				"          Column_14 as integer,",
				"          Column_15 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     wildcardPaths:['*.xlsx']) ~> ExcelSource"
			]
		}
	}
}